<%= form_for @user ,:url => user_contacts_path, :method => :post do |f| %>
  	<%#= f.error_notification %>
	  	<%= f.fields_for :contacts, @contacts do |builder| %>
	  		<%= render 'contact_fields', :f => builder %>
	  	<% end %>
	  		<%= link_to_add_fields "Add More", f, :contacts %>
	    <%= f.submit('Create Contacts', :class => "login_btn add_more ease") %>
<% end %>